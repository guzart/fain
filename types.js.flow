import theme from './theme';

export type Theme = typeof theme;

export type ClassName = string | Object;

export type ComponentProps = { theme: Theme };

export type CSSValue = any;

export type LayoutSizeName = 'xs' | 'sm' | 'md' | 'lg' | 'xl';

export type ContainerMaxWidthsName = 'sm' | 'md' | 'lg' | 'xl';

export type GridBreakpoints = { [key: LayoutSizeName]: CSSValue };

export type GridGutterWidths = { [key: LayoutSizeName]: string };

export type ContainerMaxWidths = { [key: ContainerMaxWidthsName]: string };

export type Interpolation = Function | string | number;

export type PropertyLens = (theme: Theme) => CSSValue;

export type StyleFn = (...args: any[]) => CSSValue;
